{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },

    "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq",
      "Serilog.Exceptions",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Process",
      "Serilog.Enrichers.Thread"
    ],
    "MinimumLevel": {
  "Default": "Debug",
"Override": {
  "Microsoft": "Information",
  "System": "Information",
  "Microsoft.EntityFrameworkCore.Database.Command": "Information",
  "Api": "Debug"
  }
}
    },
    "WriteTo": [
  { "Name": "Console",
    "Args": { "formatter":
     "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
  },
  { "Name": "File",
    "Args": { "path": "logs/api-.log", "rollingInterval": "Day", "shared": true }
  },
  { "Name": "Seq",
    "Args": { "serverUrl": "http://localhost:5341" }
  }
],
    "Enrich": [ "FromLogContext", "WithExceptionDetails", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": { "Application": "WebApi-Dev" },

  "Security": {
    "Pepper": ""
  },

  "JWT": {
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:5000",
    "SigningKey": ""
  },

  "AllowedHosts": "*"
}
